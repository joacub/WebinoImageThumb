{
    "name":        "joacub/webino-image-thumb",
    "description": "Image Thumbnailer for Zend Framework 2",
    "homepage":    "https://github.com/webino/WebinoImageThumb",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "keywords":   ["webino", "image", "thumb", "zf2", "zend", "framework"],
    "authors": [
        {
            "name":     "Webino, s. r. o.",
            "email":    "info@webino.sk",
            "homepage": "http://webino.sk",
            "role":     "Developer"
        },
        {
            "name":     "Peter Bacinsky",
            "email":    "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role":     "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "ext-gd": "*",
        "masterexploder/PHPThumb": "2.*",
        "zendframework/zend-di": "*"
    },
    "suggest": {
        "zendframework/zend-modulemanager": "To use it as a Zend Framework module"
    },
    "require-dev": {
        "webino/webino-dev": "dev-develop",
        "webino/webino-debug": "dev-develop",
        "zendframework/zend-modulemanager": "*",
        "zendframework/zend-version": "*",
        "zendframework/zend-console": "*",
        "zendframework/zend-loader": "*",
        "zendframework/zend-http": "*",
        "zendframework/zend-log": "*",
        "zendframework/zend-mvc": "*",
        "zendframework/zend-view": "*"
    },
    "autoload": {
        "psr-0": {"WebinoImageThumb": "src/"}
    }
}
